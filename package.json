{
    "name": "Koa2Vue2",
    "version": "0.0.1",
    "description": "Koa2 VueJs Starterkit",
    "author": "Bohdan Vovchuk <vovchuck.bogdan@gmail.com>",
    "private": false,
    "scripts": {
        "dev": "supervisor --ignore src index.js",
        "start": "cross-env NODE_ENV=production nodemon index.js",
        "build": "echo 'build:client build:server' | xargs -t -P2 -n1 yarn run && find ./dist/* -mtime +14 -type f -delete",
        "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules",
        "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules",
        "e2e": "node test/e2e/runner.js",
        "unit": "cross-env NODE_ENV=testing karma start test/unit/karma.conf.js --single-run"
    },
